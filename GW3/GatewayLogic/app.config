        <bindings>
            <basicHttpBinding>
                <binding name="DataAccessSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="DataAccessSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://caesar.psi.ch/DataAccess.asmx" binding="basicHttpBinding"
