                name="DataAccessSoap" />
            <endpoint address="http://caesar.psi.ch/DataAccess.asmx" binding="customBinding"
                bindingConfiguration="DataAccessSoap12" contract="GWLoggerSoap.DataAccessSoap"
                name="DataAccessSoap12" />
        </client>
