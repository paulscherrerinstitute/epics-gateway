<Window x:Class="GraphAnomalyVisualizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:GraphAnomalyVisualizer"
        mc:Ignorable="d"
        Title="Graph anomaly visualizer" WindowStartupLocation="CenterScreen" Height="450" Width="800">
    <Grid>
        <oxy:PlotView Model="{Binding Plot}" />
    </Grid>
</Window>