{
  "compileOnSave": false,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "noEmit": true,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015"
    ],
    "outFile": "../main.js",
    "experimentalDecorators": true
  },
  "include": [ "../Scripts/**/*.d.ts", "./**/*.ts" ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
